#include <stdio.h>
int main(){
 int position,n,i,a[100];
 printf("Enter the no. of elements in the array: \n");
 scanf("%d",&n);

 printf("Enter the elements of the array: \n");
 for(i=0; i<n; i++){
    scanf("%d",&a[i]);
    }

 n = n-1;

 printf("The modified array: \n");
    for(i = 0; i < n;i++){
    printf("%d ",a[i]);
    }
return 0;
}
